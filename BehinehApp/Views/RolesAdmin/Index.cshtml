@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>

@{
    ViewBag.Title = "کاربران مدیر";
}

<h2 class="active">لیست کاربران مدیر</h2>
<hr />
<p>
    @Html.ActionLink("افزودن", "Create",null, new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            نام
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("ویرایش", "Edit", new { id = item.Id }, new { @class = "btn btn-success" }) |
                @Html.ActionLink("جزئیات", "Details", new { id = item.Id }, new { @class = "btn btn-info" }) |
                @Html.ActionLink("حذف", "Delete", new { id = item.Id }, new { @class = "btn btn-warning" })
            </td>
        </tr>
    }

</table>
